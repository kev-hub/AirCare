@model IEnumerable<Google_maps_api.Models.City>

@{
    ViewData["Title"] = "View";
}

<h1>View</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Title)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Lng)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Oz)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NO2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SO2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CO2)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lng)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Oz)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NO2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SO2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CO2)
            </td>
            <td>]
            </td>
        </tr>
}
    </tbody>
</table>
